{
  "name": "hono-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development bun run --hot src/index.ts",
    "build": "bun build src/index.ts --target=bun --outdir=dist",
    "start": "bun ./dist/src/index.js",
    "generate:dev": "cross-env NODE_ENV=development drizzle-kit generate",
    "migrate:dev": "cross-env NODE_ENV=development drizzle-kit migrate",
    "generate:prod": "cross-env NODE_ENV=production drizzle-kit generate",
    "migrate:prod": "cross-env NODE_ENV=production drizzle-kit migrate",
    "generate:test": "cross-env NODE_ENV=test drizzle-kit generate",
    "migrate:test": "cross-env NODE_ENV=test drizzle-kit migrate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@hono/zod-openapi": "^1.1.0",
    "@scalar/hono-api-reference": "^0.9.15",
    "dotenv": "^17.2.1",
    "dotenv-expand": "^12.0.2",
    "drizzle-orm": "^0.44.4",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.9.4",
    "hono-pino": "^0.10.2",
    "install": "^0.13.0",
    "npm": "^11.5.2",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "rotating-file-stream": "^3.2.7",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@types/bun": "latest",
    "@types/pg": "^8.15.5",
    "cross-env": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.34.0",
    "eslint-plugin-format": "^1.0.1",
    "tsx": "^4.20.5"
  }
}